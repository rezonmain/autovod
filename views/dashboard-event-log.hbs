<div class="p-2 bg-neutral-900 shadow-lg border border-neutral-600 flex flex-col ">
  <h1 class="text-2xl pb-0.5">event logs</h1>
  <ul>
    {{#each events as |event|}}
    <li class="p-2 bg-neutral-800 shadow-lg border border-neutral-600">
      <div class="flex justify-between">
        <div>
          <span class="font-bold">[{{event.type}}]</span>
          <span class="text-sm">{{event.message}}</span>
        </div>
        <div>
          <pre class="text-sm">{{event.metadata}}</pre>
        </div>
      </div>
      <div class="text-sm">{{event.createdAt}}</div>
    </li>
    {{/each}}
  </ul>
</div>